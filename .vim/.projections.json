{
  "plugin/*.vim": {
    "type": "plugin",
    "alternate": [
      "after/autoload/{basename}.vim",
      "after/autoload/enable/{basename}.vim"
    ]
  },
  "after/autoload/pack.vim": {
    "type": "pack"
  },
  "after/autoload/*.vim": {
    "type": "autoload"
  },
  "colors/*.vim": {
    "type": "color"
  },
  "pack/minpac/opt/**/plugin/*.vim": {
    "type": "packplug",
    "alternate": "{file|dirname|dirname}/autoload/{basename}.vim"
  },
  "pack/minpac/opt/**/autoload/*.vim": {
    "type": "packautoload",
    "alternate": "{file|dirname|dirname}/plugin/{basename}.vim"
  }
}
